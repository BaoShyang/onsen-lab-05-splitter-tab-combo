<?xml version='1.0' encoding='utf-8'?>
<widget id="lab.onsen.splitter" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Lab-05-Splitter-Navigator-Tabbar-combo</name>
    <description>
        Onsen splitter, stack navigator and tab bar combo demo lab version
    </description>
    <author email="paulchin@hcu.edu.my" href="https://hcu.edu.my/">
        Han Chiang
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <icon src="icon144.png" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-8.0.0" />
</widget>
